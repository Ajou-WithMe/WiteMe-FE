<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".settings.Settings"
    android:clickable="true"
    android:background="#FFFFFF">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFFFF">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="match_parent"
            android:layout_height="205.5px"
            android:background="@drawable/just_bottom_line"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView21"
                android:layout_width="233px"
                android:layout_height="69px"
                android:layout_marginTop="105px"
                android:text="개인 설정"
                android:textColor="#222222"
                android:textSize="54px"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profileImage"
            android:layout_width="180px"
            android:layout_height="180px"
            android:layout_marginStart="60px"
            android:layout_marginTop="96px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <TextView
            android:id="@+id/protector"
            android:layout_width="117px"
            android:layout_height="54px"
            android:layout_marginStart="48px"
            android:layout_marginTop="127.5px"
            android:text="보호자"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toEndOf="@+id/profileImage"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <TextView
            android:id="@+id/protectorName"
            android:layout_width="wrap_content"
            android:layout_height="60px"
            android:layout_marginStart="30px"
            android:layout_marginTop="124.5px"
            android:text=""
            android:textColor="#333333"
            android:textSize="48px"
            app:layout_constraintStart_toEndOf="@+id/protector"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <TextView
            android:id="@+id/loginHow"
            android:layout_width="wrap_content"
            android:layout_height="45px"
            android:layout_marginStart="48px"
            android:layout_marginTop="210px"
            android:text=""
            android:textColor="#333333"
            android:textSize="36px"
            app:layout_constraintStart_toEndOf="@+id/profileImage"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="108px"
            android:layout_height="108px"
            android:layout_marginTop="132px"
            android:layout_marginEnd="60px"
            android:background="@drawable/change_profile"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <View
            android:id="@+id/view24"
            android:layout_width="match_parent"
            android:layout_height="24px"
            android:layout_marginTop="72px"
            android:background="#F6F6F6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/profileImage" />

        <TextView
            android:id="@+id/textView46"
            android:layout_width="144px"
            android:layout_height="57px"
            android:layout_marginStart="60px"
            android:layout_marginTop="94.5px"
            android:text="앱 설정"
            android:textColor="#333333"
            android:textSize="48px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view24" />

        <TextView
            android:id="@+id/textView47"
            android:layout_width="231px"
            android:layout_height="57px"
            android:layout_marginStart="60px"
            android:layout_marginTop="66px"
            android:text="PUSH 알림"
            android:textColor="#333333"
            android:textSize="48px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView46" />

        <com.kyleduo.switchbutton.SwitchButton
            android:id="@+id/pushAlarm"
            android:layout_width="141px"
            android:layout_height="69px"
            android:layout_marginTop="211.5px"
            android:layout_marginEnd="60px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view24" />

        <TextView
            android:id="@+id/textView48"
            android:layout_width="700px"
            android:layout_height="36px"
            android:layout_marginStart="60px"
            android:layout_marginTop="18px"
            android:text="피보호자의 위급 상황 대비를 위해 알림은 항상 켜주세요"
            android:textColor="#333333"
            android:textSize="30px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView47" />

        <TextView
            android:id="@+id/textView49"
            android:layout_width="177px"
            android:layout_height="60px"
            android:layout_marginStart="60px"
            android:layout_marginTop="144px"
            android:text="이용약관"
            android:textColor="#333333"
            android:textSize="48px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView48" />

        <ImageButton
            android:id="@+id/agreement"
            android:layout_width="108px"
            android:layout_height="108px"
            android:layout_marginTop="120px"
            android:layout_marginEnd="60px"
            android:background="@drawable/change_profile"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView48" />

        <TextView
            android:id="@+id/textView50"
            android:layout_width="177px"
            android:layout_height="60px"
            android:layout_marginStart="60px"
            android:layout_marginTop="144px"
            android:text="문의하기"
            android:textColor="#333333"
            android:textSize="48px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView49" />

        <ImageButton
            android:id="@+id/QnA"
            android:layout_width="108px"
            android:layout_height="108px"
            android:layout_marginTop="96px"
            android:layout_marginEnd="60px"
            android:background="@drawable/change_profile"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/agreement" />

        <TextView
            android:layout_width="177px"
            android:layout_height="60px"
            android:layout_marginStart="60px"
            android:layout_marginTop="120px"
            android:text="버전정보"
            android:textColor="#333333"
            android:textSize="48px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView50" />

        <TextView
            android:layout_width="150px"
            android:layout_height="60px"
            android:layout_marginTop="96px"
            android:layout_marginEnd="81px"
            android:text="v.1.0.1"
            android:textColor="#F7BD03"
            android:textSize="48px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/QnA" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/navigationView"
            android:layout_width="match_parent"
            android:layout_height="210px"
            android:layout_gravity="bottom"
            android:background="@drawable/just_bottom_line"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent">

            <ImageButton
                android:id="@+id/bulletinBoard"
                android:layout_width="108px"
                android:layout_height="108px"
                android:layout_marginStart="147px"
                android:layout_marginTop="21px"
                android:background="@drawable/bulletinboard_click"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="195px"
                android:layout_height="39px"
                android:layout_marginStart="102px"
                android:layout_marginTop="12px"
                android:text="공동돌봄게시판"
                android:textColor="#333333"
                android:textSize="29px"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/bulletinBoard" />

            <ImageButton
                android:id="@+id/group"
                android:layout_width="108px"
                android:layout_height="108px"
                android:layout_marginTop="21px"
                android:background="@drawable/group_nonclick"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="57px"
                android:layout_height="39px"
                android:layout_marginTop="12px"
                android:text="그룹"
                android:textColor="#333333"
                android:textSize="29px"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/group" />

            <ImageButton
                android:id="@+id/settings"
                android:layout_width="108px"
                android:layout_height="108px"
                android:layout_marginTop="21px"
                android:layout_marginEnd="147px"
                android:background="@drawable/settings_nonclick"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="111px"
                android:layout_height="39px"
                android:layout_marginTop="12px"
                android:layout_marginEnd="147px"
                android:text="개인설정"
                android:textColor="#333333"
                android:textSize="29px"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/settings" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>