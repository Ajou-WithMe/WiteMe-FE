<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1080px"
    android:layout_height="678px"
    android:layout_gravity="top"
    android:background="@drawable/alert_white_border">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="172px"
        android:layout_height="69px"
        android:layout_marginStart="63px"
        android:layout_marginTop="114px"
        android:text="앱 설정"
        android:textColor="#333333"
        android:textSize="54px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/xButton"
        android:layout_width="72px"
        android:layout_height="72px"
        android:layout_marginTop="114px"
        android:layout_marginEnd="60px"
        android:background="@drawable/x_button_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="234px"
        android:layout_height="48px"
        android:layout_marginStart="60px"
        android:layout_marginTop="240px"
        android:text="안심이동모드"
        android:textColor="#000000"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <com.kyleduo.switchbutton.SwitchButton
        android:id="@+id/ansim"
        android:layout_width="141px"
        android:layout_height="69px"
        android:layout_marginTop="411px"
        android:layout_marginEnd="63px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/logout"
        android:layout_width="135px"
        android:layout_height="42px"
        android:layout_marginStart="66px"
        android:layout_marginTop="108px"
        android:text="로그아웃"
        android:textColor="#000000"
        android:textSize="36px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView54" />




</androidx.constraintlayout.widget.ConstraintLayout>