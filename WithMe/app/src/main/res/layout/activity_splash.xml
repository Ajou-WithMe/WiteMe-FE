<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FED537"
    android:gravity="center"
    tools:context=".intro.SplashActivity">

    <ImageView
        android:id="@+id/with_me_logo_white_1"
        android:layout_width="330px"
        android:layout_height="186.63px"
        android:layout_marginTop="771px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/with_me_logo_white_1" />

    <ImageView
        android:id="@+id/with_me_logo"
        android:layout_width="348px"
        android:layout_height="108px"
        android:layout_marginTop="36px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/with_me_logo_white_1"
        app:srcCompat="@drawable/with_me_logo" />

</androidx.constraintlayout.widget.ConstraintLayout>