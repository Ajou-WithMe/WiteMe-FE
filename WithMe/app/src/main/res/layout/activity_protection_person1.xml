<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".group.ProtectionPersonActivity1">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="204px"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/just_bottom_line">

        <TextView
            android:id="@+id/textView35"
            android:layout_width="423px"
            android:layout_height="69px"
            android:layout_marginTop="111px"
            android:text="피보호자 정보 수정"
            android:textColor="#000000"
            android:textSize="53px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="84px"
            android:layout_height="54px"
            android:layout_marginStart="60px"
            android:layout_marginTop="120px"
            android:text="닫기"
            android:textColor="#BDBDBD"
            android:textSize="42px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/reviseProtectionPerson"
            android:layout_width="84px"
            android:layout_height="54px"
            android:layout_marginTop="120px"
            android:layout_marginEnd="60px"
            android:text="수정"
            android:textColor="#F7BD03"
            android:textSize="42px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="300px"
        android:layout_height="330px"
        android:layout_marginTop="144px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

        <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/profileImage"
            android:layout_width="300px"
            android:layout_height="300px"
            android:src="@drawable/solo_profile" />

        <ImageView
            android:id="@+id/editProfile"
            android:layout_width="96px"
            android:layout_height="96px"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:src="@drawable/edit_profile" />

    </RelativeLayout>

    <TextView
        android:id="@+id/textView32"
        android:layout_width="117px"
        android:layout_height="54px"
        android:layout_marginStart="60px"
        android:layout_marginTop="156px"
        android:text="이름"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout" />

    <EditText
        android:id="@+id/name"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginTop="120px"
        android:layout_marginEnd="60px"
        android:background="@drawable/edittext_group_description"
        android:paddingLeft="36px"
        android:text=""
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="156px"
        android:layout_height="54px"
        android:layout_marginStart="60px"
        android:layout_marginTop="105px"
        android:text="아이디"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView32" />

    <EditText
        android:id="@+id/id"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginTop="36px"
        android:layout_marginEnd="60px"
        android:background="@drawable/edittext_group_description"
        android:paddingLeft="36px"
        android:text=""
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="156px"
        android:layout_height="54px"
        android:layout_marginStart="60px"
        android:layout_marginTop="105px"
        android:text="비밀번호"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView33" />

    <LinearLayout
        android:id="@+id/password"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginTop="36px"
        android:layout_marginEnd="60px"
        android:background="@drawable/edittext_group_description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvPassword"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="******"
            android:textColor="#333333"
            android:textSize="42px"
            android:gravity="center_vertical"
            android:paddingLeft="33px"/>

    </LinearLayout>

    <TextView
        android:layout_width="117px"
        android:layout_height="54px"
        android:layout_marginStart="60px"
        android:layout_marginTop="105px"
        android:text="거주지"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <LinearLayout
        android:id="@+id/addressLayout"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginTop="36px"
        android:layout_marginEnd="60px"
        android:background="@drawable/edittext_group_description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tvAddress"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="#BDBDBD"
            android:text="거주지 검색하기"
            android:gravity="center_vertical"
            android:textSize="42px"
            android:paddingLeft="33px"
            android:maxLines="3"/>
    </LinearLayout>

    <EditText
        android:id="@+id/detailAddress"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginTop="24px"
        android:layout_marginEnd="60px"
        android:background="@drawable/edittext_group_description"
        android:hint="상세 주소 입력"
        android:textColorHint="#BDBDBD"
        android:paddingLeft="36px"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressLayout" />

    <ImageButton
        android:id="@+id/deleteProtectionPerson"
        android:layout_width="346.5px"
        android:layout_height="63px"
        android:layout_marginBottom="54px"
        android:background="@drawable/delete_account"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>