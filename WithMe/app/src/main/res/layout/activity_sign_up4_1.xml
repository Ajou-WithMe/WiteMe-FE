<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".user.SignUpActivity4_1">

    <TextView
        android:layout_width="414px"
        android:layout_height="75px"
        android:layout_marginStart="60px"
        android:layout_marginLeft="60px"
        android:layout_marginTop="321px"
        android:text="보호자 정보 입력"
        android:textColor="#222222"
        android:textSize="59px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="771px"
        android:layout_height="108px"
        android:layout_marginStart="60px"
        android:layout_marginLeft="60px"
        android:layout_marginTop="420px"
        android:text="보호자, 피보호자에게도 유용하게 쓰일거에요 정확한 정보를 입력해주세요!"
        android:textColor="#595956"
        android:textSize="41px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/nameLayout"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginStart="276px"
        android:layout_marginLeft="276px"
        android:layout_marginTop="630px"
        android:orientation="horizontal"
        android:background="@drawable/edittext_rounded_corner_sign4_not_selected"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/etName"
            android:layout_width="600px"
            android:layout_height="123px"
            android:hint="성함을 입력해주세요"
            android:textColorHint="#BDBDBD"
            android:background="@android:color/transparent"
            android:textSize="42px"
            android:paddingLeft="33px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/checkbox1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="45px"
            android:src="@drawable/check"
            android:visibility="invisible" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/phoneNumberLayout"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginStart="276px"
        android:layout_marginLeft="276px"
        android:layout_marginTop="801px"
        android:background="@drawable/edittext_rounded_corner_sign4_not_selected"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/etPhoneNumber"
            android:layout_width="600px"
            android:layout_height="123px"
            android:hint="- 없이 입력"
            android:textColorHint="#BDBDBD"
            android:textSize="42px"
            android:paddingLeft="33px"
            android:background="@android:color/transparent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/checkbox2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="45px"
            android:src="@drawable/check"
            android:visibility="invisible" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/addressLayout"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginStart="276px"
        android:layout_marginLeft="276px"
        android:layout_marginTop="972px"
        android:background="@drawable/edittext_rounded_corner_sign4_not_selected"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/etAddress1"
            android:layout_width="600px"
            android:layout_height="wrap_content"
            android:text="거주지 검색하기"
            android:textColor="#BDBDBD"
            android:layout_gravity="center"
            android:maxLines="3"
            android:textSize="42px"
            android:paddingLeft="33px" />


        <ImageView
            android:id="@+id/checkbox3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="45px"
            android:src="@drawable/check"
            android:visibility="invisible" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/detailAddressLayout"
        android:layout_width="744px"
        android:layout_height="123px"
        android:layout_marginStart="276px"
        android:layout_marginLeft="276px"
        android:layout_marginTop="24px"
        android:background="@drawable/edittext_rounded_corner_sign4_not_selected"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressLayout">

        <EditText
            android:id="@+id/etDetailAddress"
            android:layout_width="600px"
            android:layout_height="123px"
            android:background="@android:color/transparent"
            android:gravity="top"
            android:hint="상세주소 입력"
            android:paddingLeft="33px"
            android:paddingTop="36px"
            android:paddingRight="33px"
            android:textColorHint="#BDBDBD"
            android:textSize="42px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/checkbox4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="45px"
            android:src="@drawable/check"
            android:visibility="invisible" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="78px"
        android:layout_height="54px"
        android:layout_marginStart="60px"
        android:layout_marginLeft="60px"
        android:layout_marginTop="666px"
        android:text="성함"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="117px"
        android:layout_height="54px"
        android:layout_marginStart="60px"
        android:layout_marginLeft="60px"
        android:layout_marginTop="837px"
        android:text="연락처"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="117px"
        android:layout_height="54px"
        android:layout_marginStart="60px"
        android:layout_marginLeft="60px"
        android:layout_marginTop="1008px"
        android:text="거주지"
        android:textColor="#333333"
        android:textSize="42px"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="591px"
        android:layout_height="39px"
        android:layout_marginTop="12px"
        android:layout_marginEnd="180px"
        android:layout_marginRight="180px"
        android:text="보호자의 실제 거주지를 정확하게 입력해주세요!"
        android:textColor="#FF302B"
        android:textSize="29px"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detailAddressLayout" />

    <Button
        android:id="@+id/startWithMe"
        android:layout_width="960px"
        android:layout_height="141px"
        android:layout_marginBottom="72px"
        android:background="@drawable/radius_1_nonclickable"
        android:clickable="false"
        android:text="With Me 시작하기"
        android:textColor="#BDBDBD"
        android:textSize="42px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>