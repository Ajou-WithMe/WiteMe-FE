<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    tools:context=".settings.KakaoProfile">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFFFF">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="match_parent"
            android:layout_height="205.5px"
            android:background="@drawable/just_bottom_line"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/close"
                android:layout_width="78px"
                android:layout_height="54px"
                android:layout_marginStart="60px"
                android:layout_marginTop="120px"
                android:text="닫기"
                android:textColor="#BDBDBD"
                android:textSize="42px"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="315px"
                android:layout_height="69px"
                android:layout_marginTop="105px"
                android:text="개인정보 수정"
                android:textColor="#222222"
                android:textSize="54px"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/revise"
                android:layout_width="78px"
                android:layout_height="54px"
                android:layout_marginTop="120px"
                android:layout_marginEnd="60px"
                android:text="수정"
                android:textColor="#F7BD03"
                android:textSize="42px"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="204px"
            android:layout_height="200px"
            android:layout_marginStart="60px"
            android:layout_marginTop="97px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

            <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/profileImage"
                android:layout_width="180px"
                android:layout_height="180px" />

            <ImageView
                android:id="@+id/editProfile"
                android:layout_width="96px"
                android:layout_height="96px"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:src="@drawable/edit_profile" />

        </RelativeLayout>

        <TextView
            android:id="@+id/protector"
            android:layout_width="117px"
            android:layout_height="54px"
            android:layout_marginStart="48px"
            android:layout_marginTop="127.5px"
            android:text="보호자"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toEndOf="@+id/relativeLayout"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <TextView
            android:id="@+id/protectorName"
            android:layout_width="wrap_content"
            android:layout_height="60px"
            android:layout_marginStart="30px"
            android:layout_marginTop="124.5px"
            android:text=""
            android:textColor="#333333"
            android:textSize="48px"
            app:layout_constraintStart_toEndOf="@+id/protector"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <TextView
            android:id="@+id/loginHow"
            android:layout_width="wrap_content"
            android:layout_height="45px"
            android:layout_marginStart="48px"
            android:layout_marginTop="210px"
            android:text=""
            android:textColor="#333333"
            android:textSize="36px"
            app:layout_constraintStart_toEndOf="@+id/relativeLayout"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <TextView
            android:id="@+id/textView51"
            android:layout_width="117px"
            android:layout_height="54px"
            android:layout_marginStart="60px"
            android:layout_marginTop="405px"
            android:text="이메일"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <View
            android:id="@+id/view26"
            android:layout_width="744px"
            android:layout_height="123px"
            android:layout_marginTop="369px"
            android:layout_marginEnd="60px"
            android:background="@drawable/radius_1_settings"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

        <TextView
            android:id="@+id/textView52"
            android:layout_width="156px"
            android:layout_height="54px"
            android:layout_marginStart="60px"
            android:layout_marginTop="117px"
            android:text="비밀번호"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView51" />

        <View
            android:id="@+id/view25"
            android:layout_width="744px"
            android:layout_height="123px"
            android:layout_marginTop="48px"
            android:layout_marginEnd="60px"
            android:background="@drawable/radius_1_settings"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view26" />

        <TextView
            android:id="@+id/textView53"
            android:layout_width="156px"
            android:layout_height="54px"
            android:layout_marginStart="60px"
            android:layout_marginTop="117px"
            android:text="연락처"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView52" />

        <EditText
            android:id="@+id/phoneNumber"
            android:layout_width="744px"
            android:layout_height="123px"
            android:layout_marginTop="48px"
            android:layout_marginEnd="60px"
            android:paddingLeft="33px"
            android:background="@drawable/edittext_rounded_corner_sign4_not_selected"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view25" />

        <TextView
            android:layout_width="117px"
            android:layout_height="54px"
            android:layout_marginStart="60px"
            android:layout_marginTop="120px"
            android:text="거주지"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView53" />

        <LinearLayout
            android:id="@+id/addressLayout"
            android:layout_width="744px"
            android:layout_height="123px"
            android:layout_marginTop="48px"
            android:layout_marginEnd="60px"
            android:background="@drawable/edittext_rounded_corner_sign4_not_selected"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/phoneNumber">

            <TextView
                android:id="@+id/tvAddress"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:maxLines="3"
                android:paddingLeft="33px"
                android:paddingRight="33px"
                android:text="거주지 입력하기"
                android:textColor="#BDBDBD"
                android:textSize="42px" />

        </LinearLayout>

        <EditText
            android:id="@+id/etDetailAddress"
            android:layout_width="744px"
            android:layout_height="123px"
            android:layout_marginTop="24px"
            android:layout_marginEnd="60px"
            android:background="@drawable/edittext_rounded_corner_sign4_not_selected"
            android:hint="상세 주소 입력"
            android:textColorHint="#BDBDBD"
            android:paddingLeft="36px"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/addressLayout" />

        <TextView
            android:id="@+id/logout"
            android:layout_width="156px"
            android:layout_height="54px"
            android:layout_marginStart="624px"
            android:layout_marginTop="144px"
            android:text="로그아웃"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etDetailAddress" />

        <TextView
            android:id="@+id/quit"
            android:layout_width="156px"
            android:layout_height="54px"
            android:layout_marginStart="84px"
            android:layout_marginTop="144px"
            android:text="회원탈퇴"
            android:textColor="#333333"
            android:textSize="42px"
            app:layout_constraintStart_toEndOf="@+id/logout"
            app:layout_constraintTop_toBottomOf="@+id/etDetailAddress" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>